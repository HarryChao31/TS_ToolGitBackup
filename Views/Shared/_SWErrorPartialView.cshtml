@model List<SeamlessWalletError>

<style>
    /* 自定義表格邊框樣式 */
    .table-bordered {
        border: 2px solid #000; /* 將邊框寬度設置為2px，顏色為黑色 */
    }

    /* 自定義表頭的背景顏色和文字顏色 */
    .table-dark th {
        background-color: #343a; /* 表頭背景色 */
        color: #fff; /* 表頭文字顏色 */
    }
</style>
<h2>SeamlesWalletError</h2>
<table class="table table-bordered" id="resultTable">
    <thead class="table-dark">
        <tr>
            <th scope="col">Webid</th>
            <th scope="col">Action</th>
            <th scope="col">ErrorMessage</th>
            <th scope="col">HostName</th>
            <th scope="col">Request</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.WebId)</td>
                <td>@Html.DisplayFor(modelItem => item.Action)</td>
                <td>@Html.DisplayFor(modelItem => item.ErrorMessage)</td>
                <td>@Html.DisplayFor(modelItem => item.HostName)</td>
                <td>@Html.DisplayFor(modelItem => item.Request)</td>
            </tr>
        }
    </tbody>
</table>
</div>
<div>
    @Html.ActionLink("", "", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>